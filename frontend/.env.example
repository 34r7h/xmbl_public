# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890abcdef
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=XMBL App Generator
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=A powerful app generator platform
VITE_APP_URL=http://localhost:5173

# Environment
VITE_NODE_ENV=development
VITE_DEBUG=true

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_DEV_TOOLS=true

# External Services
VITE_VERCEL_URL=
VITE_SENTRY_DSN=
VITE_POSTHOG_API_KEY=
VITE_INTERCOM_APP_ID=

# Theme Configuration
VITE_DEFAULT_THEME=system
VITE_BRAND_COLOR=#3B82F6
VITE_ACCENT_COLOR=#10B981

# Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/*,application/pdf

# Social Authentication (if enabled)
VITE_GOOGLE_CLIENT_ID=
VITE_GITHUB_CLIENT_ID=
VITE_DISCORD_CLIENT_ID=

# Development Only
VITE_MOCK_API=false
VITE_SHOW_DEVTOOLS=true
VITE_LOG_LEVEL=debug
