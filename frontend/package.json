{
  "name": "@xmbl/frontend",
  "version": "1.0.0",
  "description": "Frontend application for XMBL App Generator",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue",
    "vite",
    "typescript",
    "app-generator",
    "frontend"
  ],
  "author": "XMBL Team",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "@vueuse/core": "^10.5.0",
    "@vueuse/components": "^10.5.0",
    "axios": "^1.6.2",
    "firebase": "^10.6.0",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "tailwindcss": "^3.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "vue-draggable-plus": "^0.3.5",
    "vue-toastification": "^2.0.0-rc.5",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.1",
    "prismjs": "^1.29.0",
    "monaco-editor": "^0.44.0",
    "chart.js": "^4.4.0",
    "vue-chartjs": "^5.2.0",
    "sortablejs": "^1.15.0",
    "vue3-colorpicker": "^2.3.0",
    "perfect-scrollbar": "^1.5.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.0",
    "typescript": "~5.2.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^9.0.7",
    "@types/prismjs": "^1.26.3",
    "@types/sortablejs": "^1.15.7",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
